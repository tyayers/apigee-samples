<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<TargetEndpoint name="TargetV1-Healthy">
  <Description/>
  <FaultRules/>
  <PreFlow name="PreFlow">
    <Request/>
    <Response/>
  </PreFlow>
  <PostFlow name="PostFlow">
    <Request/>
    <Response/>
  </PostFlow>
  <Flows/>
  <HTTPTargetConnection>
    <Properties/>
    <LoadBalancer>
      <Algorithm>RoundRobin</Algorithm>
      <Server name="target1"/>
      <Server name="target2">
        <IsFallback>true</IsFallback>
      </Server>
      <ServerUnhealthyResponse>
        <ResponseCode>429</ResponseCode>
      </ServerUnhealthyResponse>
    </LoadBalancer>
    <Path>/v1/process</Path>
  </HTTPTargetConnection>
</TargetEndpoint>
